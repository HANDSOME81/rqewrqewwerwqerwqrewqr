game.Players.LocalPlayer.CharacterAdded:Connect(function()
wait(1)
game:GetService("Players").LocalPlayer.Character["Local Ragdoll"]:Destroy()
game:GetService("Players").LocalPlayer.Character["State Handler"]:Destroy()
game:GetService("Players").LocalPlayer.Character.RightAnkleJoint:Destroy()
game:GetService("Players").LocalPlayer.Character.RootJoint:Destroy()
game:GetService("Players").LocalPlayer.Character.WaistJoint:Destroy()
game:GetService("Players").LocalPlayer.Character.NeckJoint:Destroy()
game:GetService("Players").LocalPlayer.Character.LeftShoulderJoint:Destroy()
game:GetService("Players").LocalPlayer.Character.RightShoulderJoint:Destroy()
game:GetService("Players").LocalPlayer.Character.LeftElbowJoint:Destroy()
game:GetService("Players").LocalPlayer.Character.RightElbowJoint:Destroy()
game:GetService("Players").LocalPlayer.Character.LeftWristJoint:Destroy()
game:GetService("Players").LocalPlayer.Character.RightWristJoint:Destroy()
game:GetService("Players").LocalPlayer.Character.LeftHipJoint:Destroy()
game:GetService("Players").LocalPlayer.Character.RightHipJoint:Destroy()
game:GetService("Players").LocalPlayer.Character.LeftKneeJoint:Destroy()
game:GetService("Players").LocalPlayer.Character.RightKneeJoint:Destroy()
game:GetService("Players").LocalPlayer.Character.LeftAnkleJoint:Destroy()
print("Ragdoll System Is Bypassed")
end)
